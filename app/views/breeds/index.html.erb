<div class="container">
<div class="dogs-card-title m-2 text-center">
  <h1>It's a match!</h1>
  <h3>Browse through the list of your perfect Pawtners:</h3>
</div>
<div class="dogs-card-container p-3">
    <% @breeds.each do |breed| %>
      <div class="dogs-card p-1">
        <div class="dog-image">
          <%= image_tag(breed.picture) %>
        </div>
        <div class="card-content">
          <h5><%= breed.name %></h5 class="text-center">
          </div>
      </div>
    <% end %>
  </div>
</div>
