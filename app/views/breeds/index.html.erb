<div class="dogs-card-title m-2 text-center bolder">
  <h1>It's a match!</h1>
  <h3>Browse through the list of your perfect Pawtners:</h3>
</div>
<%= link_to "Return to dashboard", dashboard_path, class: "btn-dash" %>
<div class="breeds-container">
<div class="dogs-card-container p-3">
    <% @breed_names.each do |breed| %>
      <div class="dogs-card p-1">
        <div class="dog-image">
          <%= link_to breed_path(breed), class: "card-link" do %>
          <%= image_tag(breed.picture) %>
        </div>
        <div class="card-content">
          <h3 class="bold"><%= breed.name %></h3>
        </div>
      </div>
        <% end %>
    <% end %>
  </div>
</div>
